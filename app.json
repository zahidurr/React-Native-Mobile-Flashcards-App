{
  "name": "quizapp",
  "displayName": "quizapp",
  "expo": {
    "name": "quizapp",
    "slug": "quizapp",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.zahidur.quizapp"
    },
    "android": {
      "package": "me.zahidur.quizapp",
      "versionCode": 2
    },
    "description": "A React Native app for iOS, Android and Web that allows users to create decks cards and play quiz.",
    "githubUrl": "https://github.com/zahidurr/React-Native-Quiz-App",
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}